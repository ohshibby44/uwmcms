  RewriteCond %{REQUEST_URI} !^/locations/otolaryngology-head-and-neck-surgery-center-uwmc/?.*
  RewriteCond %{REQUEST_URI} !^/locations/otolaryngology-head-and-neck-surgery-harborview/?.*
  RewriteCond %{REQUEST_URI} !^/services/ear-nose-and-throat-care/?.*
  RewriteCond %{REQUEST_URI} !^/services/otolaryngology/?.*
  # End ENT #
  # Start Heart Institute #
  RewriteCond %{REQUEST_URI} !^/locations/cardiology-eastside-specialty-center/?.*
  RewriteCond %{REQUEST_URI} !^/locations/cardiology-edmonds/?.*
  RewriteCond %{REQUEST_URI} !^/locations/cardiology-harborview/?.*
  RewriteCond %{REQUEST_URI} !^/locations/cardiology-uwmc/?.*
  RewriteCond %{REQUEST_URI} !^/locations/cardiology-imaging-echocardiography-ecg-nuclear-medicine-uwmc/?.*
  RewriteCond %{REQUEST_URI} !^/locations/summit-cardiology-ballard/?.*
  RewriteCond %{REQUEST_URI} !^/locations/summit-cardiology-nwhmc/?.*
  RewriteCond %{REQUEST_URI} !^/locations/cardiac-fitness-northwest/?.*
  RewriteCond %{REQUEST_URI} !^/services/heart-institute/?.*
  RewriteCond %{REQUEST_URI} !^/services/cardiology/?.*
  # End Heart Institute #
  # Start Eye Institute #
  RewriteCond %{REQUEST_URI} !^/services/eye-institute/?.*
  RewriteCond %{REQUEST_URI} !^/locations/eye-center-uwmc/?.*
  RewriteCond %{REQUEST_URI} !^/locations/eye-institute-harborview/?.*
  RewriteCond %{REQUEST_URI} !^/locations/eyes-on-james/?.*
  RewriteCond %{REQUEST_URI} !^/locations/ophthalmology-harborview/?.*
  # End Eye Institue #
  RewriteRule ^(.*)$ - [CO=uwm-proxied-page-visit:true:%{HTTP_HOST}:0:/,C]
  RewriteRule ^(.*)$ https://www2.uwmedicine.org%{REQUEST_URI} [L,P]

  # Redirects for launched pages. These pages are served from Drupal.
  RewriteCond %{HTTP_HOST} ^www.uwmedicine(test)?.org
  RewriteRule ^locations/(.*)/directions/?(.*)?$ "https://www.uwmedicine.org/locations/$1#directions-tab" [NE,L,R=301]
  RewriteRule ^locations/(.*)/care-providers/?(.*)?$ "https://www.uwmedicine.org/locations/$1#providers-tab" [NE,L,R=301]
  RewriteRule ^services/urgent-care/(providers|locations)$ "https://www.uwmedicine.org/services/urgent-care" [L,R=301]
  RewriteRule "^services/urgent-care/(.+)#911-calls$" "https://www.uwmedicine.org/services/urgent-care#block-emergencymedicine" [NE,L,R=301]
  RewriteRule ^services/urgent-care/(.+)$ "https://www.uwmedicine.org/locations/primary-care-$1" [L,R=301]

</IfModule>
